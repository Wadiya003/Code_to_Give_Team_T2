<body>
    
    <div class="logo"><a href="#"><img
        src="data:image/png;base64,"
        height="40px" width="120px"></a></div>
<nav>
<ul>
    <li><a href="/login">Home</a></li>
    <li><a href="/register">Register</a></li>
    <li><a href="/admin-login">Admin</a></li>
</ul>

